Cell[BoxData[GridBox[{
   {"", Cell[TextData[{
     Cell[BoxData[
      RowBox[{"EscapeProbabilityRun", "[", 
       StyleBox[
        RowBox[{"LAMBDAfile", ",", " ", 
         ButtonBox["Nc",
          BaseStyle->"Link",
          ButtonData->"paclet:ref/Nc"], ",", " ", 
         ButtonBox["FWHM",
